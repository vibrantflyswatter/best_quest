    "eslint": "eslint",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VladVes/project-lvl3-s130.git"
  },
