    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "./dist/",
      "./node_modules/",
      "./src/bin",
      "./coverage"
    ]
  },
  "version": "1.1.6",
  "description": "page downloader",
  "main": "dist/index.js",
  "bin": {
